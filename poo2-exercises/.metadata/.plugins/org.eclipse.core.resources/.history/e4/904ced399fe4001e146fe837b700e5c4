package entities;

import java.time.LocalDate;

public class Matricula {

	private Aluno aluno;
	private Curso curso;
	private LocalDate dataMatricula;
	
	public Matricula(Aluno aluno, Curso curso, LocalDate dataMatricula) {
		this.aluno = aluno;
		this.curso = curso;
		this.dataMatricula = dataMatricula;
	} 
	
	
}
